<form class="SearchBox" action="/search">
  <div class="SearchBoxInput">
    <input
      type="text"
      name="q"
      value="<%= it?.data?.query || '' %>"
    >
  </div>
  <div class="SearchBoxButtonContainer">
    <button class="SearchBoxButton">
      <%~ include('/includes/svg/SearchBoxButton.eta') %>
    </button>
  </div>
</form>